{
    "name": "passionweb/form-after-submit",
    "type": "typo3-cms-extension",
    "description": "Shows the usage of the hook 'afterSubmit' of EXT:form, for example to validate entered form data. (TYPO3 CMS)",
    "version": "1.0.0",
    "homepage": "https://www.passionweb.de",
    "authors": [
        {
            "name": "Manuel Schnabel",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^12.4",
        "typo3/cms-form": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "Passionweb\\FormAfterSubmit\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/form-after-submit": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "form_after_submit"
        }
    }
}
